    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-livereload-plugin": "^2.3.0"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  },
  "nodecg": {
    "compatibleRange": "^1.8.0",
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.0.0"
    },
    "dashboardPanels": [
      {
